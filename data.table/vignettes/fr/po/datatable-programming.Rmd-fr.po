"     inner = \"square\",\n"
"     var1 = \"Sepal.Length\",\n"
"     var2 = \"Sepal.Width\",\n"
"     out = \"Sepal.Hypotenuse\",\n"
"     filter_col = \"Species\",\n"
"     filter_val = I(c(\"versicolor\", \"virginica\")),\n"
